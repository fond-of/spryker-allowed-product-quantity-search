{
  "name": "fond-of-spryker/allowed-product-quantity-search",
  "description": "Allowed product quantity search module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.1",
    "spryker/event": "^1.0.0 || ^2.0.0",
    "spryker/product-page-search": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "spryker/product-page-search-extension": "^1.2.0",
    "fond-of-spryker/allowed-product-quantity": "dev-master"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.12",
    "codeception/codeception": "^2.4",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "php-coveralls/php-coveralls": "^2.1",
    "fond-of-codeception/spryker": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
